  "dependencies": {
    "@koa/cors": "^3.0.0",
    "@koa/router": "^8.0.2",
    "dotenv": "^8.2.0",
    "got": "^11.8.5",
    "koa": "^2.11.0",
    "koa-logger": "^3.2.1",
    "concurrently": "^5.0.0",
    "nodemon": "^2.0.1"
  },
